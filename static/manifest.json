{
  "manifest_version": 2,
  "name": "SubGrabber for youtube",
  "version": "1.0",
  "description": "exports your youtube subs to the takeout csv format",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/feed/channels*"
      ],
      "js": [
        "./content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "permissions": [
    "downloads"
  ]
}