{
  "manifest_version": 2,
  "name": "SubGrabber for youtube",
  "version": "1.0",
  "description": "exports your youtube subs to the takeout csv format",
  "icons": [
    "48": "icon.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/"
      ],
      "js": [
        "./content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "permissions": [
    "downloads"
  ]
}